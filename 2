#include <iostream>

using namespace std;

int main() {
    int score;

    cout << "-------GRADING SYSTEM-------" << endl;
    cout << "Type in the Test Score: ";
    cin >> score;

    if (score >= 0 && score <= 100) { // 유효한 점수 범위를 확인합니다.
        if (score >= 90 && score <= 100) {
            cout << "A" << endl;
        } else if (score >= 80 && score <= 89) {
            cout << "B" << endl;
        } else if (score >= 70 && score <= 79) {
            cout << "C" << endl;
        } else if (score >= 60 && score <= 69) {
            cout << "D" << endl;
        } else {
            cout << "F" << endl;
        }
    } else {
        cout << "Invalid Score" << endl;
    }

    return 0;
}
